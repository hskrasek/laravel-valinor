{
    "name": "hskrasek/laravel-valinor",
    "type": "library",
    "description": "",
    "version": "0.0.1",
    "homepage": "https://github.com/hskrasek/laravel-valinor",
    "authors": [
        {
            "name": "Hunter Skrasek",
            "email": "hunterskrasek@me.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "cuyz/valinor": "^1.12",
        "illuminate/contracts": "^11.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.17",
        "orchestra/testbench": "^9.3",
        "pestphp/pest": "^2.35",
        "pestphp/pest-plugin-laravel": "^2.3",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.2",
        "phpstan/phpstan-phpunit": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "HSkrasek\\LaravelValinor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HSkrasek\\LaravelValinor\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-hskrasek-laravel-valinor --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "scripts-descriptions": {

    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "HSkrasek\\LaravelValinor\\ValinorServiceProvider"
            ],
            "aliases": {
                "Mapper": "HSkrasek\\LaravelValinor\\Facades\\Mapper"
            }
        }
    }
}
